<h1>Feedback Listing</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Email</th>
      <th>Subject</th>
      <th>Description</th>
      <th>First name</th>
      <th>Last name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.email %></td>
        <td><%= feedback.subject %></td>
        <td><%= feedback.description %></td>
        <td><%= feedback.first_name %></td>
        <td><%= feedback.last_name %></td>
        <td><%= link_to 'Show', feedback %></td>
        <td><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
        <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feedback', new_feedback_path %>
